{
  "name": "zipcode2kml",
  "version": "1.0.0",
  "description": "An application for converting zipcodes into kml files for overlaying on maps",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack zipcode2kml-darwin-x64/zipcode2kml.app/Contents/Resources/app zipcode2kml-darwin-x64/zipcode2kml.app/Contents/Resources/app.asar",
    "build": "electron-packager . zipcode2kml && cp Icon.icns zipcode2kml-darwin-x64/zipcode2kml.app/Contents/Resources/electron.icns"
  },
  "repository": "https://github.com/ehardacre/zipcode2kml",
  "keywords": [
    "Maps",
    "zipcode",
    "kml"
  ],
  "author": "Peak Studios",
  "license": "CC0-1.0",
  "devDependencies": {
    "asar": "^2.1.0",
    "electron": "^11.5.0",
    "electron-packager": "^14.2.1",
    "fs-extra": "^8.1.0"
  }
}
